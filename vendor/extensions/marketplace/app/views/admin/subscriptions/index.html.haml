%h1 Subscriptions

%table#subscriptions.index
  %thead
    %th id
    %th reader
    %th type
    %th begins_on
    %th expires_on
    %th paid_on
    %th cancelled_on
    %th show
    %th edit
  %tbody
    -@subscriptions.each do |sub|
      %tr
        %td=sub.id
        %td=sub.reader.name if sub.reader
        %td=sub.membership_type
        %td=sub.begins_on
        %td=sub.expires_on
        %td=sub.order.paid_on if sub.order
        %td=sub.cancelled_on
        %td=link_to 'show', admin_subscription_path(sub)
        %td=link_to 'edit', edit_admin_subscription_path(sub)
