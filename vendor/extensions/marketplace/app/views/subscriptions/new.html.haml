-if RAILS_ENV == 'test'
  -# because we dont have critical layouts in test env we need this to test
  =render :partial => 'load_js_libs_badtime'

%h1 New Subscription
=stylesheet_link_tag 'select2'
- case @subscription.membership_type
- when 'casual'
  =render 'subscriptions/casual_membership_form'
- when 'full'
  =render 'subscriptions/full_membership_form'
- else
  =render 'subscriptions/select_membership_type_form'



