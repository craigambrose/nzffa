<h1>Listing user_branch_roles</h1>

<table>
  <tr>
    <th>User</th>
    <th>Role</th>
    <th>Branch</th>
  </tr>

<% @user_branch_roles.each do |user_branch_role| %>
  <tr>
    <td><%=h user_branch_role.user_id %></td>
    <td><%=h user_branch_role.role_id %></td>
    <td><%=h user_branch_role.branch_id %></td>
    <td><%= link_to 'Show', user_branch_role %></td>
    <td><%= link_to 'Edit', edit_user_branch_role_path(user_branch_role) %></td>
    <td><%= link_to 'Destroy', user_branch_role, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New user_branch_role', new_user_branch_role_path %>