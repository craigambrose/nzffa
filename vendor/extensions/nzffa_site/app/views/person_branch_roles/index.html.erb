<h1>Listing person_branch_roles</h1>

<table>
  <tr>
    <th>Person</th>
    <th>Role</th>
    <th>Branch</th>
  </tr>

<% @person_branch_roles.each do |person_branch_role| %>
  <tr>
    <td><%=h person_branch_role.person_id %></td>
    <td><%=h person_branch_role.role_id %></td>
    <td><%=h person_branch_role.branch_id %></td>
    <td><%= link_to 'Show', person_branch_role %></td>
    <td><%= link_to 'Edit', edit_person_branch_role_path(person_branch_role) %></td>
    <td><%= link_to 'Destroy', person_branch_role, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New person_branch_role', new_person_branch_role_path %>