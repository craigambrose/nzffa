<% title "Person Sessions" %>

<table>
  <tr>
    <th>Email</th>
    <th>Password</th>
  </tr>
  <% for person_session in @person_sessions %>
    <tr>
      <td><%=h person_session.email %></td>
      <td><%=h person_session.password %></td>
      <td><%= link_to "Show", person_session %></td>
      <td><%= link_to "Edit", edit_person_session_path(person_session) %></td>
      <td><%= link_to "Destroy", person_session, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New Person Session", new_person_session_path %></p>
